<div class="blog-post">
    <div class="blog-post--header">
        <h1 class="blog-post--title"><%= posts[n].title %> </h1>
    </div>
    <div class="blog-post--info">
        <p>
            posted <%= posts[n].date %> by William Martin
        </p>
    </div>
    <div>
        <img class="blog-post--image" src="https://via.placeholder.com/700x500" alt="">
    </div>
    <div class="blog-post--body">
        <%- posts[n].body %>
    </div>
</div>